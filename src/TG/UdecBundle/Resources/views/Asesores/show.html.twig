{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Asesores</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Director</th>
                <td>{{ entity.director }}</td>
            </tr>
            <tr>
                <th>Jurado</th>
                <td>{{ entity.jurado }}</td>
            </tr>
            <tr>
                <th>Asesmetd</th>
                <td>{{ entity.asesmetd }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('asesores') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('asesores_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
